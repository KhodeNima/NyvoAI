   <property name="styleSheet">
    <string notr="true">QCheckBox {
  width: 35px;
  height: 35px;
  cursor: pointer;
  margin-right: -22px;
  appearance: none;
  border-radius: 5px;
  background-color: #6593cf;
  z-index: 2;
  transition: all 0.3s;
}

.container {
  width: 35px;
  height: 35px;
  position: relative;
  top: 4px;
  left: -8%;
  border-radius: 5px;
  background-color: #6593cf;
  transition: all 0.3s;
}

QCheckBox::before {
  content: &quot;&quot;;
  background-color: #6593cf;
  position: relative;
  display: flex;
  top: 45%;
  left: 50%;
  width: 55px;
  height: 3px;
  border-radius: 25px;
  transform: translate(100px, 0px) scale(0);
  transition: ease-out 0.15s;
}

QCheckBox:checked::before {
  transform: translateX(2em);
  top: 12px;
  transition: ease-out 0.15s;
}

QcheckBox:hover {
  transform: translate(4px, 4px);
  transition: ease-out 0.15s;
  background-color: #6593cf;
}

QCheckBox:checked {
  transform: translate(4px, 4px);
  transition: ease-out 0.15s;
  background-color: #6593cf;
}

.svg-icon {
  position: absolute;
  width: 25px;
  height: 25px;
  display: flex;
  z-index: 3;
  top: 35%;
  left: 11%;
  color: #fefefe;
  font-family: &quot;Gill Sans&quot;, &quot;Gill Sans MT&quot;, Calibri, &quot;Trebuchet MS&quot;, sans-serif;
  transform: rotate(0deg) scale(0);
  transition: ease-in 0.2s;
}

QCheckBoxchecked ~ .svg-icon {
  transform: rotate(360deg) scale(1);
  transition: ease-in 0.2s;
}
</string>
